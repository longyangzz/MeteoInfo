<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dclw.ncforecast.mapper.ReadGribToSqlMapper">


    <select id="inserGribDataToSql" resultType="Integer">
       INSERT INTO [dbo].[Netcdf_EDA10]
           ([GenTime]
           ,[StartForecastTime]
           ,[ForecastTime]
           ,[UWind]
           ,[VWind]
           ,[Lon]
           ,[lat]
           ,[STCD])
         VALUES  ${stateValues}
    </select>
</mapper>
